#include<stdio.h>
#include<math.h>
int main()
{
    int n,i,res;
    printf("enter total no of elements in an array");
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    int key;
    printf("enter the key:");
    scanf("%d",&key);
    res=jump_search(a,key,n);
    if(res==-1)
    printf("key not found");
    else
    printf("key found");
}
int jump_search(int a[],int key,int n)
{
int start=0,end=sqrt(n);
while(a[end]<key&&start<end)
{
    start=end;
    end=end+sqrt(n);
    if(end>(n-1))
    end=n-1;
}
for(int i=start;i<=end;i++)
{
    if(a[i]==key)
    return 1;
}
    return -1;
}
