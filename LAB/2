#include<stdio.h>
int main()
{
    int i,n,t,k,j;
    printf("input total no of comparisons");
    scanf("%d",&t);
    for(i=0;i<t;i++)
    {
        int s=0,flag=0;
        printf("enter total no of elements in an array");
        scanf("%d",&n);
        int a[n];
        for(j=0;j<n;j++)
        {
            scanf("%d",&a[j]);
        }
        printf("enter searched element");
        scanf("%d",&s);
        for(k=0;k<n;k++)
        {
           if(a[k]==s)
           {
               flag=1;
               printf("Present %d",k+1);
               break;
           }
        }
        if(flag==0)
        printf("Not Present %d",s);
    }
}
